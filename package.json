{
  "name": "income-expense-tracker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently -k \"npm:backend\" \"npm:frontend\"",
    "frontend": "ng serve",
    "backend": "npm --prefix backend run dev",
    "build": "ng build",
    "build:all": "ng build && npm run build --prefix backend",
    "start:prod": "npm start --prefix backend",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "bootstrap": "^5.3.8",
    "echarts": "^6.0.0",
    "ngx-echarts": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.1",
    "@types/pg": "^8.16.0"
  }
}